{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "jsx": "react",
      "outDir": "./dist",
      "strict": true,
      "declaration": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "noUnusedLocals": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true
    }
  }
  